{
  "name": "deepcode",
  "//": "Changing display name requires change in general.ts",
  "displayName": "DeepCode (Beta)",
  "description": "Detect bugs and quality issues in JavaScript, TypeScript, React, Vue and more...",
  "version": "0.0.1",
  "icon": "images/deepCodeLogo.jpg",
  "publisher": "deepcode",
  "homepage": "https://www.deepcode.ai/",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepCodeAI/vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "code analysis",
    "java",
    "javascript",
    "typescript",
    "python"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "DeepCode",
        "properties": {
          "deepcode.review.results.hideInformationIssues": {
            "type": "boolean",
            "default": false,
            "description": "Hide information issues",
            "scope": "window"
          },
          "deepcode.api.cloudBackend": {
            "type": "string",
            "default": "",
            "description": "Deepcode cloud backend",
            "scope": "application"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "deepcode.start",
        "title": "DeepCode"
      },
      {
        "command": "deepcode.settings",
        "title": "DeepCode Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "watch": "tsc -watch -p ./",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/nock": "^11.1.0",
    "@types/node": "^10.12.21",
    "nock": "^11.7.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@types/git-repo-name": "^1.0.0",
    "@types/git-username": "^1.0.0",
    "@types/mz": "^0.0.32",
    "@types/request-promise": "^4.1.44",
    "git-repo-name": "^1.0.1",
    "git-username": "^1.0.0",
    "ignore": "^5.1.4",
    "mz": "^2.7.0",
    "open": "^6.4.0",
    "request-promise": "^4.2.4"
  }
}
